{
  "name": "brainfuck-interpreter",
  "version": "1.0.0",
  "description": "Brainfuck interpreter with Typescript",
  "main": "main.ts",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "contributors": [
    "Adri√°n Ferrera <adrian.fergon@leanmind.es>",
    "Daniel Ramos <daniel.ramos@leanmind.es>"
  ],
  "license": "ISC",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "babel src -x .ts -d build",
    "build:watch": "babel src -x .ts -d build --watch",
    "lint": "tslint -t stylish --project .",
    "pretest": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/jest": "^23.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/__test__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ]
  }
}
